  <div class=\"header-inner clearfix\">\n        <div class=\"movie-info-left\">\n          <div class=\"avatar-shadow\">\n            <el-image class=\"avatar\" :src=\"cinemaInfo.cinemaPicture[0]\"\n                      :preview-src-list=\"cinemaInfo.cinemaPicture\">\n            </el-image>\n          </div>\n        </div>\n        <div class=\"movie-info-right\">\n          <div class=\"movie-info-msg\">\n            <br/>\n            <h1 class=\"movie-name\">{{cinemaInfo.cinemaName}}</h1>\n            <br/>\n            <div class=\"phone-number\">{{cinemaInfo.cinemaAddress}}</div>\n            <div class=\"phone-number\">电话: {{cinemaInfo.cinemaPhone}}</div>\n            <div style=\"margin: 25px 0;\">影院服务 ————————————————————</div>\n            <div class=\"service\"><span class=\"service-tag\">免费停车</span>观影前1小时至观影后1小时内免费停车</div>\n            <div class=\"service\"><span class=\"service-tag\">领快乐水</span>节假日期间，每张票可领取一瓶快乐水</div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"movie-info-detail-container\">\n      <div class=\"movie-info-detail clearfix\">\n        <div class=\"main-content\">\n          <div class=\"crumbs-nav-container\">\n            <a href=\"/welcome\">panda电影</a> &gt; <a href=\"/aboutUs\"> {{cinemaInfo.cinemaName}}</a>\n          </div>\n          <!-- 影院正在上映的所有电影 -->\n          <div class=\"movie-list-container\"\n               v-if=\"cinemaInfo.sysMovieList !== null\n                      && cinemaInfo.sysMovieList.length !== 0\">\n            <span class=\"scroll-prev scroll-btn\" @click=\"prevPage\"></span>\n            <span class=\"scroll-next scroll-btn\" @click=\"nextPage\"></span>\n            <div class=\"movie-list\" ref=\"movieListRef\" :style=\"'left:' + left + 'px'\">\n              <div class=\"movie\"\n                   v-for=\"item in cinemaInfo.sysMovieList\"\n                   :key=\"item.movieId\"\n                   :class=\"{active: item.movieId === activeMovie}\"\n                   @click=\"activeMovie = item.movieId\">\n                <img :src=\"global.base + JSON.parse(item.moviePoster)[0]\"/>\n              </div>\n            </div>\n          </div>\n\n          <!-- 当前选中电影信息 -->\n          <div class=\"show-list\"\n               v-if=\"cinemaInfo.sysMovieList !== null\n                      && cinemaInfo.sysMovieList.length !== 0\">\n            <div class=\"show-movie-info\">\n              <div>\n                <h2 class=\"show-movie-name\">{{movieDict[activeMovie].movieName}}</h2>\n              </div>\n              <!-- 电影描述 -->\n              <div class=\"show-movie-desc\">\n                <div>\n                  <span class=\"key\">时长 : </span>\n                  <span>{{movieDict[activeMovie].movieLength}}分钟</span>\n                </div>\n                <div>\n                  <span class=\"key\">类型 : </span>\n                  <span>{{movieDict[activeMovie].movieCategoryList.join(' / ')}}</span>\n                </div>\n              </div>\n            </div>\n            <!-- 展示场次的日期 -->\n            <div class=\"show-date\" v-if=\"sessions !== null && sessions.length !== 0\">\n              <span>观影时间 :</span>\n              <span class=\"date-item\"\n                    :class=\"{active: activeDate === key}\"\n                    @click=\"activeDate = key\"\n                    v-for=\"(item, key) in sessionDict\"\n                    :key=\"key\">{{calculateDate(key)}}</span>\n            </div>\n            <div class=\"session-info\">\n              <el-table\n                  :data=\"sessionDict[activeDate]\"\n                  stripe\n                  style=\"width: 100%\">\n                <el-table-column\n                    label=\"放映时间\">\n                  <template slot-scope=\"scope\">\n                    <span class=\"begin-time\">{{scope.row.playTime}}</span>\n                    <br/>\n                    <span class=\"end-time\">{{scope.row.endTime}}散场</span>\n                  </template>\n                </el-table-column>\n                <el-table-column\n                    prop=\"languageVersion\"\n                    label=\"语言版本\">\n                </el-table-column>\n                <el-table-column\n                    label=\"放映厅\">\n                  <template slot-scope=\"scope\">\n                    {{scope.row.sysHall.hallName}}\n                  </template>\n                </el-table-column>\n                <el-table-column\n                    label=\"影厅提示\">\n                  <template slot-scope=\"scope\">\n                    {{scope.row.sessionTips === null ? '无' : scope.row.sessionTips}}\n                  </template>\n                </el-table-column>\n                <el-table-column\n                    label=\"售价\">\n                  <template slot-scope=\"scope\">\n                    <span class=\"sell-price\"><span class=\"stonefont\">{{scope.row.sessionPrice}}</span></span>\n                  </template>\n                </el-table-column>\n                <el-table-column\n                    label=\"选座购票\">\n                  <template slot-scope=\"scope\">\n                    <el-button type=\"primary\" round @click=\"toChooseSeat(scope.row.sessionId)\">选座购票</el-button>\n                  </template>\n                </el-table-column>\n              </el-table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport moment from 'moment'\nexport default {\n  name: \"CinemaInfo\",\n  data() {\n    return {\n      cinemaId: this.$route.params.cinemaId,\n      cinemaInfo: {\n        user: {},\n        cinemaPicture: [],\n        sysMovieList: []\n      },\n      //用于获取指定id的电影信息\n      movieDict: { 0: {\n          movieBoxOffice: 0,\n          movieCategoryList: [],\n          movieId: 0,\n          movieIntroduction: '',\n          movieLength: 0,\n          movieName: '',\n          moviePictures: '',\n          moviePoster: '',\n          releaseDate